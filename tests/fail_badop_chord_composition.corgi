// Setting int to pitch
int main() {
    pitch p1;
    pitch p2;
    duration d1;
    duration d2;
    chord c1;
    chord c2;
    track t1;
    track t2;
    composition comp;
    
    p1 = 4;
    p2 = 5;
    d1 = $1/4$;
    d2 = $1/8$;
    c1 = [(p1,d1), (p1,d2), (p2, d2)];
    c2 = [(p1,d1), (p1,d2), (p2, d2), (p2, d1)];
    t1 = [c1, c2, c2];
    t2 = [c1, c1];
    comp = [t1, t2, t1];

    c1 = comp;
}